  "repository": {
    "type": "git",
    "url": "https://github.com/nbelthan/oscillyx.git"
  },
  "homepage": "https://oscillyx.vercel.app",
  "dependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3", 
    "hardhat": "^2.17.2",
    "hardhat-gas-reporter": "^1.0.9",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2"
  },
  "engines": {
