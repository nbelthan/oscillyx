{
  "name": "oscillyx",
  "version": "1.0.0",
  "description": "Revolutionary Blockchain Physics NFT Collection - World's first NFT with rarity determined by actual blockchain characteristics",
  "main": "hardhat.config.js",
  "scripts": {
    "compile": "npx hardhat compile",
